{
  "name": "@e11community/envii",
  "version": "1.0.2",
  "packageManager": "yarn@3.2.1",
  "main": "index.js",
  "bin": "index.js",
  "files": [
    "angle",
    "square",
    "env/*",
    "**/*.js"
  ],
  "scripts": {
    "build": "tsc -p .",
    "prepack": "tsc -p .",
    "publish": "yarn npm publish --access public",
    "angle": "node . angle",
    "square": "node . square",
    "angle:demo": "node . --env=demo angle",
    "square:demo": "node . --env=demo square",
    "freeball": "node .",
    "start": "node . nest start",
    "husky": "node . husky"
  },
  "repository": {
    "url": "https://github.com/e11community/envii",
    "type": "git"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18.0.3",
    "husky": "^8.0.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
